/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;
    // Out = ^AB + A^B
    PARTS:
    Not(in=a,out=nota);
    And(a=nota,b=b,out=out1);
    Not(in=b,out=notb);
    And(a=a,b=notb,out=out2);
    Or(a=out1,b=out2,out=out);
}