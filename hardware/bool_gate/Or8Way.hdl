/**
 * 8-way Or: 
 * out = (in[0] or in[1] or ... or in[7])
 */

CHIP Or8Way {
    IN in[8];
    OUT out;

    PARTS:
    Or(a=in[0],b=in[1],out=o1);
    Or(a=o1,b=in[2],out=o2);
    Or(a=o2,b=in[3],out=o3);
    Or(a=in[4],b=in[5],out=o4);
    Or(a=o4,b=in[6],out=o5);
    Or(a=o5,b=in[7],out=o6);
    Or(a=o3,b=o6,out=out);
}